<%- include("header") -%>



        <div class="box" id="heading">
            <h1>It's a <%= listTitle %>
            </h1>
        </div>

        <div class="box">
            <% for(var i=0; i<items.length;i++){ %>
                <div class="item">
                    <input type="checkbox" />
                    <p>
                        <%= items[i] %>
                    </p>
                </div>
                <% } %>


                    <form action="/" method="post" class="item">
                        <input type="text" name="item" placeholder="New Item">
                        <button type="submit" name="list" value=<%=listTitle %>>+</button>
                    </form>
        </div>
    <%- include("footer") -%>
